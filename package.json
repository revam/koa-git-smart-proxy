  "author": "Mikal Stordal <mikalstordal@gmail.com> (https://github/revam)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/revam/koa-git-proxy/issues"
  },
  "homepage": "https://github.com/revam/koa-git-proxy#readme",
  "devDependencies": {
    "@types/grunt": "^0.4.22",
    "@types/http-status": "^0.2.30",
    "@types/koa": "^2.0.43",
    "@types/koa-send": "^4.1.1",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "@types/through": "0.0.29",
    "grunt": "^1.0.1",
    "grunt-mocha": "^1.0.4",
    "grunt-ts": "^6.0.0-beta.17",
    "grunt-tslint": "^5.0.1",
    "mocha": "^4.1.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "git-side-band-message": "0.0.3",
    "http-status": "^1.0.1",
    "koa-send": "^4.1.2",
    "through": "^2.3.8"
  }
