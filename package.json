  "types": "index.d.ts",
  "scripts": {
    "lint": "grunt lint",
    "test": "grunt test",
    "build": "grunt build",
    "build-grunt": "tsc GruntFile.ts --target esnext --moduleResolution node --module commonjs",
    "cleanup": "grunt cleanup",
    "prelint": "npm run build-grunt && grunt prelint",
    "pretest": "npm run lint && grunt pretest",
    "prebuild": "npm run test && grunt prebuild",
    "prepublish": "npm run build && grunt run prebublish",
    "postpublish": "grunt postpublish"
